import java.util.Scanner;
import java.text.*;
public class Task8 {
	public static void main(String [] args) {
		Scanner inputStr = new Scanner(System.in);
		System.out.println("¬ведите дату в формате dd.mm.yyyy: ");
		String dateStr = inputStr.nextLine();
		System.out.println(isValid(dateStr));
		inputStr.close();
	}
	private static final DateFormat DATA_FORMAT = new SimpleDateFormat("dd.mm.yyyy");
	static {
		DATA_FORMAT.setLenient(true);
	}
	private static boolean isValid(String date) {
		try {
			return DATA_FORMAT.format(DATA_FORMAT.parse(date)).equals(date);
		} catch (ParseException ex) {
			return false;
		}
	}
}