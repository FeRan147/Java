import java.util.Scanner;
public class Task11 {
	public static void main(String [] args) {
		System.out.println(" вадраты чисел от 0 до 10:");
		for(int i = 0; i <= 10; i++) {
			System.out.print(i + "\t");
		}
		System.out.println();
		for(int i = 0; i <= 10; i++) {
			System.out.print((i * i) + "\t");
		}
	}
}